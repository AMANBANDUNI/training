<h1>Contacts</h1>

<%# debugger %>
<table>
  <thead>
  	<th>Name</th>
  	<th>Phone</th>
  	<th>Email</th>
  	<th>Address</th>
  	<th>Image</th>
  </thead>
  <tbody>
  	<% current_user.contacts.each do |contact| %>
  	<tr>
  		<td><%= contact.name %></td>
  		<td><%= contact.phone_number %></td>
  		<td><%= contact.email %></td>
  		<td><%= contact.address %></td>
  		<td><%= contact.image %></td>
      <td><%= link_to 'show', contact_path(contact) %></td>
      <td><%= link_to 'edit', edit_contact_path(contact) %></td>
  		<td><%= link_to 'delete', contact, method: :delete, data: {confirm: "Are you sure ??"} %></td>
  	</tr>
  	<% end %>
  </tbody>
</table>
<br>
<%= link_to 'Add New Contact', new_contact_path %>
<br>
<%= link_to 'back', :back %>
<br>
<%= link_to 'Home Page', '/' %>